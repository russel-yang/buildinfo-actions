  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "ncc build index.js -o .dist",
    "postversion": "npm run build"
  },
  "repository": "github:russel-yang/buildinfo-actions",
  "author": "Take-Two Interactive, Inc.",
  "license": "UNLICENSED",
  "dependencies": {
    "@actions/core": "1.2.6"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.24.1"
